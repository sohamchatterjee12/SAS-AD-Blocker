{
    "name" : "SAS AdBlocker",
    "version" : "1.0",
    "description" : "SAS AdBlocker",
    "permissions" : ["webRequest", "webRequestBlocking","tabs","storage" , "activeTab", "<all_urls>"],
    "background" : {
        "scripts" : ["background.js"]
    },
    "icons" : {
        "16" : "icons/logo_final.png",
        "48" : "icons/logo_final.png",
        "128" : "icons/logo_final.png"
    },
    "manifest_version" : 2,
    "browser_action": {
        "default_icon": "icons/logo_final.png",
        "default_title": "Hello",
        "default_popup": "popup.html"
      }
}